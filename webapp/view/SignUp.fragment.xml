<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:forms="sap.ui.layout.form">
	<Dialog id="idSignUpDialog" title="Employee Details" >
		<forms:SimpleForm editable="true" maxContainerCols="2" id="ComplaisId" labelSpanL="5" labelSpanM="8" labelSpanS="6" adjustLabelSpan="false"
			emptySpanL="0" emptySpanM="0" emptySpanS="0" layout="ResponsiveLayout" columnsL="2" columnsM="2">
			<Label text="Employee Id"/>
			<Input id="idEmpId" value=""/>
			<Label text="Employee Name"/>
			<Input id="idEmpName" value=""/>
			<Label text="Employee Designation"/>
			<Input id="idEmpDesig" value=""/>
			<Label text=" Password"/>
			<Input id="idPass" value=""/>
			<Label text="Email"/>
			<Input id="idEmail" value=""/>
			<Label text="Phone No" />
			<Input id="idNum" value=""/>
			<Label text="System No"/>
			<Input id="idSysNo" value=""/>
			<Label text="D-O-B"/>
			<Input id="idDob" value=""/>
			</forms:SimpleForm>
		<beginButton>
			<Button text="SignUp" press="onSignup"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="onSubmit"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>